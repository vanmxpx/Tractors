@if(!(  utm_content.ToString() == "mototraktor_kupit_vd" || 
        utm_content.ToString() == "mototraktor_kupit_v2" ||
        utm_content.ToString() == "mototraktor_dostavka_v2" ||
        utm_content.ToString() == "mototraktor_kupit_vd-ua" || 
        utm_content.ToString() == "mototraktor_kredit_v2" ||
        utm_content.ToString() == "mototraktor_lviv_v2" ||
        utm_content.ToString() == "mototraktor_kupit_video") 
        && utm_content.video != null) {

<script>


        $(document).ready(function() {
            $(window).scroll(function(e){
        
                var pos1=$("#video1").offset().top-300;
        
                var scrollTop = window.pageYOffset ? window.pageYOffset : (document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop);
        
                if (scrollTop>=pos1 && $( window ).width() > 500){
                    $(".play-video-btn").click();
                    document.getElementById('video1').volume=0;
                }
            });
            
            
            $('.play-video-btn').on('click', function(event){
                event.preventDefault();
                var self = $(this);
                self.fadeOut('slow');
                self.siblings('.video-block').fadeIn('slow', function(){
                    var myVideo = document.getElementById("video1"); 
                    myVideo.play();
                });
            });
        });
    
</script>

