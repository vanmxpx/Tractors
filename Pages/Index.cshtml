@{
    string informationStyle = "";
    if(@utm_content.s2_this_bu != null)
    {
        informationStyle += "padding-top: 2.3rem; padding-bottom: 2.4rem;";
    }
}
<section id="information" 
        class="lazy well-xl text-sm-left bg-image bg-fixed bg-image-2"
        data-src="@utm_content.s2_background"
        style='@informationStyle'>
    <div class="container">

        @if( utm_content.s2_this_bu_title != null ) { 
            <h1 class="s2_this_bu_title">@Html.Raw(utm_content.s2_this_bu_title)</h1>
        }

        <div class="row">

            @if( utm_content.s2_this_bu != null ) {

                    <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 text-center s2_this_bu wow bounceInLeft" data-wow-delay="0.5s">
                        <img src="images/minitraktor_bu/bu_s1.png">
                        @Html.Raw(utm_content.s2_this_bu[0])
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 text-center s2_this_bu wow bounceInLeft" data-wow-delay="0.2s">
                        <img src="images/minitraktor_bu/bu_s2.png">
                        @Html.Raw(utm_content.s2_this_bu[1])
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 text-center s2_this_bu wow bounceInRight" data-wow-delay="0.2s">
                        <img src="images/minitraktor_bu/bu_s3.png">
                        @Html.Raw(utm_content.s2_this_bu[2])
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3 text-center s2_this_bu wow bounceInRight" data-wow-delay="0.5s">
                        <img src="images/minitraktor_bu/bu_s4.png">
                        @Html.Raw(utm_content.s2_this_bu[3])
                    </div>

                    <div class="s2_this_but">
                        <a href="#recall-dialog" class="btn btn-md btn-primary bt2 popup-with-zoom-anim ">@Html.Raw(utm_content.s2_this_left_but)</a>
                    </div>
            }
            else {
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 text-left wow bounceInLeft" data-wow-delay="0.5s">
                    <div class="s2-box-left">
                        <div class="s2-box-title">@Html.Raw(utm_content.s2_left_title)</div>
                        @foreach (var value in utm_content.s2_this_left)
                        {
                            <div class="this2" style='@utm_content.s2_left_this2_style'> <div class="this-line this-img"><img src="images/v.png"  alt=""></div> <div class="this-line this-text">@value</div> </div>
                        }
                        <div class="this_but">
                                <a href="#recall-dialog" class="btn btn-md btn-primary bt2 popup-with-zoom-anim ">@Html.Raw(utm_content.s2_this_left_but)</a>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 text-left wow bounceInRight" data-wow-delay="1s">
                    <div class="s2-box-right" style='@utm_content.s2_right_style'>
                        <div class="s2-box-title">@Html.Raw(utm_content.s2_right_title)</div>
                        @foreach (var value in utm_content.s2_this_right)
                        {
                            <div class="this2" style='@utm_content.s2_right_this2_style'> <div class="this-line this-img"><img src="images/x.png"  alt=""></div> <div class="this-line this-text">@value</div> </div>
                        }
                    </div>
                </div>
            }
        </div>
    </div>
</section>

